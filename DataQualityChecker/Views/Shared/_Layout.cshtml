                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">DataQualityChecker</a>
            </div>
        </nav>
    </header>
    -->

    <div class="form-group header">

    </div>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

